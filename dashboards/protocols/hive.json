                  "value": "30m"
               },
               {
                  "selected": false,
                  "text": "1h",
                  "value": "1h"
               },
               {
                  "selected": false,
                  "text": "6h",
                  "value": "6h"
               },
               {
                  "selected": false,
                  "text": "12h",
                  "value": "12h"
               },
               {
                  "selected": false,
                  "text": "1d",
                  "value": "1d"
               },
               {
                  "selected": false,
                  "text": "7d",
                  "value": "7d"
               },
               {
                  "selected": false,
                  "text": "14d",
                  "value": "14d"
               },
               {
                  "selected": false,
                  "text": "30d",
                  "value": "30d"
               }
            ],
            "query": "1m,5m,30m,1h,6h,12h,1d,7d,14d,30d",
            "type": "interval"
         }
      ]
   },
   "time": {
      "from": "now-1h",
      "to": "now"
   },
   "timezone": "browser",
   "title": "Hive"
}
