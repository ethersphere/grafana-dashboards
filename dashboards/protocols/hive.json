{
   "description": "",
   "graphTooltip": 1,
   "panels": [
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "description": "",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 0,
                  "showPoints": "never"
               },
               "decimals": 0,
               "unit": "percentunit"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v10.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "increase(bee_hive_unreachable_peers_count{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval])/(increase(bee_hive_reachable_peers_count{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval])+increase(bee_hive_unreachable_peers_count{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval]))",
               "intervalFactor": 1,
               "legendFormat": "{{instance}}"
            }
         ],
         "title": "Reachable error rate",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "description": "",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 0,
                  "showPoints": "never"
               }
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 0
         },
         "id": 2,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v10.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "increase(bee_hive_reachable_peers_count{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval])+increase(bee_hive_unreachable_peers_count{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval])",
               "intervalFactor": 1,
               "legendFormat": "{{instance}} unreachable peers"
            }
         ],
         "title": "Reach attemps",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "description": "Time taken for ping operation",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 8
         },
         "id": 3,
         "interval": "1m",
         "options": {
            "cellGap": 2,
            "color": {
               "mode": "scheme",
               "scheme": "Blues",
               "steps": 128
            },
            "legend": {
               "show": false
            }
         },
         "pluginVersion": "v10.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(rate(bee_hive_ping_time_bucket{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval]))by(le)",
               "intervalFactor": 1,
               "legendFormat": "{{le}}"
            }
         ],
         "title": "Ping time",
         "type": "heatmap"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "description": "Time taken for failed ping operation",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 8
         },
         "id": 4,
         "interval": "1m",
         "options": {
            "cellGap": 2,
            "color": {
               "mode": "scheme",
               "scheme": "Blues",
               "steps": 128
            },
            "legend": {
               "show": false
            }
         },
         "pluginVersion": "v10.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(rate(bee_hive_fail_ping_time_bucket{cluster=~\"$cluster\", namespace=~\"$namespace\", instance=~\"$instance\"}[$interval]))by(le)",
               "intervalFactor": 1,
               "legendFormat": "{{le}}"
            }
         ],
         "title": "Failed ping times",
         "type": "heatmap"
      }
   ],
   "schemaVersion": 36,
   "tags": [
      "protocols"
   ],
   "templating": {
      "list": [
         {
            "name": "datasource",
            "query": "prometheus",
            "type": "datasource"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "multi": true,
            "name": "cluster",
            "query": "label_values(bee_info, cluster)",
            "refresh": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "multi": true,
            "name": "namespace",
            "query": "label_values(bee_info{cluster=~\"$cluster\"}, namespace)",
            "refresh": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "multi": true,
            "name": "instance",
            "query": "label_values(bee_info{cluster=~\"$cluster\", namespace=~\"$namespace\"}, instance)",
            "refresh": 1,
            "type": "query"
         },
         {
            "auto": true,
            "auto_count": 30,
            "auto_min": "1s",
            "current": {
               "selected": false,
               "text": "auto",
               "value": "$__auto_interval_interval"
            },
            "name": "interval",
            "options": [
               {
                  "selected": true,
                  "text": "auto",
                  "value": "$__auto_interval_interval"
               },
               {
                  "selected": false,
                  "text": "1m",
